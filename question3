#include <iostream>
using namespace std;

int main()
{
    int number=0;
    cout << "Digite Um numero: ";
    cin << number;

    if (number % 2 == 0){
        cout << "Este Numero e Par!: " "\n";
    }else {
        cout << "Esse Numero e Impar!: " "\n";

    }
    
    system ("pause");

    return 0;
    
}
