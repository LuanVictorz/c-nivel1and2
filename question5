#include <iostream>
using namespace std;

int main(){
	int number1 = 0;
    int number2 = 0;
    int sum = 0;

cout << "Digite o primeiro numero: \n";
cin >> number1;
cout << "Digite o segundo numero: \n";
cin >> number2;
sum = number1 + number2;

if (sum > 20){
	cout << sum + 8 <<"\n";
	}else {
		cout << sum -5 <<"\n";
	}

    system ("pause");

    return 0;
    
}
